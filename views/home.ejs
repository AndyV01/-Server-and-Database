<body class="stars">
    <header>
        <div>
            <img class="logo" src="./pics/Logo fondo transparente[617].png" alt="Logo Kush">
            <nav>
                <ul>
                    <li> <a class="nav-button" href="/blog">BLOG</a> </li>
                    <li> <a class="nav-button" href="/catalogo">CATALOGO</a> </li>
                    <li> <a class="nav-button" href="Foro">FORO</a> </li>
                    <li> <a class="nav-button" href="Contactos">CONTACTOS</a> </li>
                </ul>
            </nav>
        </div>
        <img id="baner" src="./pics/led-para-marihuana.jpg" alt="baner">
    </header>
    <span>
        <img id="user" src="/pics/villain.png" alt="user">
        <form id="login" action="http://localhost:4000/login" method="POST">
            <input class="input" type="email" name="usuario" placeholder="mail">
            <input class="input" type="password" name="password" placeholder="password">
            <button class="button-login">Iniciar secion</button>
        </form>
